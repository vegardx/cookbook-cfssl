{
  "signing": {
    "profiles": {
      <% node['cfssl']['profiles'].each_with_index do |(key, val), i| %>
      "<%= key %>": {
        "auth_key": "<%= key %>",
        "expiry": "<%= val['expiry'] %>",
        "usages": <%= val['usages'] %>
      }<%= "," if i < (node['cfssl']['profiles'].size - 1) %>
      <% end %>
    }
  },
  "auth_keys": {
    <% node['cfssl']['profiles'].each_with_index do |(key, val), i| %>
    "<%= key %>": {
      "key": "<%= val['key'] %>",
      "type": "<%= val['type'] %>"
    }<%= "," if i < (node['cfssl']['profiles'].size - 1) %>
    <% end %>
  }
}
